    {
      "name": "mdv-api",
      "envs": [
        "ENV",
        "APP_URL",
        "DATABASE_URL",
        "REDIS_URL",
        "PAYSTACK_PUBLIC_KEY",
        "PAYSTACK_SECRET_KEY",
        "RESEND_API_KEY",
        "CLOUDINARY_URL",
        "JWT_SECRET",
        "OTEL_EXPORTER_OTLP_ENDPOINT",
